@prefix TS28317_RanScNrm: <http://ericsson.com/models/3gpp/TS28317/RanScNrm#> .
@prefix TS28623_ComDefs: <http://ericsson.com/models/3gpp/TS28623/ComDefs#> .
@prefix TS28623_GenericNrm: <http://ericsson.com/models/3gpp/TS28623/GenericNrm#> .
@prefix dash: <http://datashapes.org/dash#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

[] a sh:NodeShape ;
    sh:property [ a sh:PropertyShape ;
            sh:node dash:ListShape ;
            sh:path TS28317_RanScNrm:nEInformation ;
            sh:property [ sh:class TS28317_RanScNrm:NEInfomration ;
                    sh:path ( [ sh:zeroOrMorePath rdf:rest ] rdf:first ) ] ],
        [ a sh:PropertyShape ;
            sh:class TS28623_ComDefs:Uri ;
            sh:path TS28317_RanScNrm:configDataFileLocation ] ;
    sh:targetClass TS28317_RanScNrm:ScMgmtProfile_Single .

[] a sh:NodeShape ;
    sh:node dash:ListShape ;
    sh:property [ sh:class TS28317_RanScNrm:Sc_Process_Single ;
            sh:path ( [ sh:zeroOrMorePath rdf:rest ] rdf:first ) ] ;
    sh:targetClass TS28317_RanScNrm:Sc_Process_Multiple .

[] a sh:NodeShape ;
    sh:class TS28623_GenericNrm:Top ;
    sh:targetClass TS28317_RanScNrm:Sc_Process_Single .

[] a sh:NodeShape ;
    sh:property [ a sh:PropertyShape ;
            sh:class TS28623_ComDefs:DateTime ;
            sh:path TS28317_RanScNrm:endTime ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:integer ;
            sh:maxInclusive 100 ;
            sh:minInclusive 0 ;
            sh:path TS28317_RanScNrm:progressPercentage ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:path TS28317_RanScNrm:jobId ],
        [ a sh:PropertyShape ;
            sh:path TS28317_RanScNrm:resultStateInfo ;
            sh:xone [ a sh:PropertyShape ;
                    sh:datatype xsd:string ],
                [ a sh:PropertyShape ;
                    sh:datatype xsd:string ;
                    sh:in ( "UNKNOWN" "INCORRECT_CONFIGURATION" "NE_HARDWARE_ERROR_DELECTED" "DISCONNECTION_BETWEEN_NE_AND_OAM" "OTHER" ) ] ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:in ( "NE_HEALTH_CHECK" "SW_DOWNLOAD" "SW_INSTALLATION" "SW_ACTIVATION" "PREPARE_BASIC_CONFIGURATION_AND_OAMLINK" "RETRIEVE_CONFIGURATION_DATA" "SETUP_PRECONFIGURED_SIGNALLING_LINKS" "SET_FINAL_STATE_OF_NE" ) ;
            sh:path TS28317_RanScNrm:progressStateInfo ],
        [ a sh:PropertyShape ;
            sh:class TS28623_ComDefs:DateTime ;
            sh:path TS28317_RanScNrm:startTime ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:in ( "NOT_STARTED" "RUNNING" "FINSHED" "FAILED" "CANCELLING" "CANCELLED" ) ;
            sh:path TS28317_RanScNrm:status ] ;
    sh:targetClass TS28317_RanScNrm:ScProcessMonitor .

[] a sh:NodeShape ;
    sh:class TS28623_GenericNrm:Top ;
    sh:targetClass TS28317_RanScNrm:ScMgmtProfile_Single .

[] a sh:NodeShape ;
    sh:or_ [ a sh:PropertyShape ;
            sh:class TS28317_RanScNrm:NEIdentification ],
        [ a sh:PropertyShape ;
            sh:class TS28317_RanScNrm:NEType ] ;
    sh:targetClass TS28317_RanScNrm:NEInfomration .

[] a sh:NodeShape ;
    sh:property [ a sh:PropertyShape ;
            rdfs:comment "Setting this attribute to \"TRUE\" cancels the self configuration process. " ;
            sh:datatype xsd:string ;
            sh:in ( true false ) ;
            sh:path TS28317_RanScNrm:cancelScProcess ],
        [ a sh:PropertyShape ;
            sh:class TS28317_RanScNrm:NEIdentification ;
            sh:path TS28317_RanScNrm:nEIdentification ],
        [ a sh:PropertyShape ;
            sh:class TS28317_RanScNrm:ScProcessMonitor ;
            sh:path TS28317_RanScNrm:scProcessMonitor ],
        [ a sh:PropertyShape ;
            sh:class TS28623_ComDefs:Dn ;
            sh:path TS28317_RanScNrm:scMgmtProfileRef ] ;
    sh:targetClass TS28317_RanScNrm:Sc_Process_Single .

[] a sh:NodeShape ;
    sh:targetClass TS28317_RanScNrm:NEIdentification ;
    sh:xone [ a sh:PropertyShape ;
            sh:class TS28623_ComDefs:Dn ],
        [ a sh:PropertyShape ;
            sh:class TS28317_RanScNrm:EnbId ],
        [ a sh:PropertyShape ;
            sh:class TS28317_RanScNrm:GnbId ] .

[] a sh:NodeShape ;
    sh:targetClass TS28317_RanScNrm:resources_RanScNrm ;
    sh:xone [ a sh:PropertyShape ;
            sh:class TS28317_RanScNrm:ScMgmtProfile_Single ],
        [ a sh:PropertyShape ;
            sh:class TS28317_RanScNrm:Sc_Process_Single ] .

[] a sh:NodeShape ;
    sh:property [ a sh:PropertyShape ;
            sh:class TS28317_RanScNrm:Sc_Process_Multiple ;
            sh:path TS28317_RanScNrm:Sc_Process ],
        [ a sh:PropertyShape ;
            sh:class TS28317_RanScNrm:ScMgmtProfile_Multiple ;
            sh:path TS28317_RanScNrm:ScMgmtProfile ] ;
    sh:targetClass TS28317_RanScNrm:SubNetwork_ncO_RanScNrm .

[] a sh:NodeShape ;
    sh:node dash:ListShape ;
    sh:property [ sh:class TS28317_RanScNrm:ScMgmtProfile_Single ;
            sh:path ( [ sh:zeroOrMorePath rdf:rest ] rdf:first ) ] ;
    sh:targetClass TS28317_RanScNrm:ScMgmtProfile_Multiple .

