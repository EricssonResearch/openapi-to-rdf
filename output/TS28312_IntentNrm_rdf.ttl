    rdfs:domain TS28312_IntentNrm:ExpectationTarget ;
    rdfs:range TS28312_IntentNrm:Condition .

TS28312_IntentNrm:targetContexts a rdf:Property ;
    rdfs:comment "It describes the list of constraints and conditions that should apply for a specific expectationTarget." ;
    rdfs:domain TS28312_IntentNrm:ExpectationTarget ;
    rdfs:range TS28312_IntentNrm:Context .

TS28312_IntentNrm:targetFulfilmentInfo a rdf:Property ;
    rdfs:domain TS28312_IntentNrm:TargetFulfilmentResult ;
    rdfs:range TS28312_IntentNrm:FulfilmentInfo .

TS28312_IntentNrm:targetFulfilmentResult a rdf:Property ;
    rdfs:domain TS28312_IntentNrm:ExpectationFulfilmentResult ;
    rdfs:range TS28312_IntentNrm:TargetFulfilmentResult .

TS28312_IntentNrm:targetName a rdf:Property ;
    rdfs:domain TS28312_IntentNrm:ExpectationTarget,
        TS28312_IntentNrm:TargetFulfilmentResult ;
