               ] ;
  cpack:primaryRepository [ a cpack:GitRepository ;
                            cpack:gitURL <https://gitlab.internal.ericsson.com/er-brazil/yaml-to-rdf/yaml_importer.git>
                          ] ;
  cpack:requires [ cpack:name "reasoner" ] ;
  cpack:description "OpenAPI yaml importer" .
